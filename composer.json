{
    "name": "terminal42/contao-fineuploader",
    "description": "FineUploader bundle for Contao Open Source CMS",
    "keywords": ["contao", "uploader", "fineuploader", "chunking"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [{
        "name": "terminal42 gmbh",
        "homepage": "https://www.terminal42.ch"
    }],
    "funding": [{
        "type": "github",
        "url": "https://github.com/terminal42"
    }, {
        "type": "other",
        "url": "https://ko-fi.com/terminal42"
    }],
    "support": {
        "issues": "https://github.com/terminal42/contao-fineuploader/issues",
        "source": "https://github.com/terminla42/contao-fineuploader"
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "contao/core-bundle": "^4.4",
        "codefog/contao-haste": "^4.17"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/php-cs-fixer": "^2.2"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\FineUploaderBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\FineUploaderBundle\\ContaoManager\\Plugin"
    }
}
